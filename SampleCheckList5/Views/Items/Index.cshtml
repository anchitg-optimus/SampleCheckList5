@model IEnumerable<SampleCheckList5.Models.Item>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Project.CheckList_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Items_Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Project.CheckList_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Items_Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Items_ID }) |
            @Html.ActionLink("Details", "Details", new { id = item.Items_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Items_ID }) 
           
        </td>
    </tr>
}

</table>
